   "source": [
    "def change(n):\n",
    "    count = 0\n",
    "    coins = [500, 100, 50, 10]\n",
    "    for coin in coins:\n",
    "        count += n // coin\n",
    "        n %= coin\n",
    "    return count\n",
    "\n",
    "n = int(input())\n",
    "change(n)"
   ]
