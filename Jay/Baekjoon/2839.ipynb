   "source": [
    "n = int(input())\n",
    "count = 0\n",
    "while n >= 0 :\n",
    "    if n % 5 == 0 :  \n",
    "        count += (n // 5)  \n",
    "        print(count)\n",
    "        break\n",
    "    else :\n",
    "        n -= 3  \n",
    "        count += 1  \n",
    "else:\n",
    "    print(-1)"
   ]
  }
