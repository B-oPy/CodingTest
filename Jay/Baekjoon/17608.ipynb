   "source": [
    "n = int(input())\n",
    "list = []\n",
    "for _ in range(n):\n",
    "    list.append(int(input()))\n",
    "    \n",
    "cnt = 1\n",
    "k = list[-1]\n",
    "for i in range(n-1, -1, -1):\n",
    "  if list[i] > k :\n",
    "    cnt += 1\n",
    "    k = list[i]\n",
    "print(cnt)"
   ]
  }
